/dts-v1/;

#include "meson-s4.dtsi"

/ {
	model = "Amlogic Meson S4 AQ222 Development Board";
	compatible = "amlogic,aq222", "amlogic,s4";
	interrupt-parent = <&gic>;
	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		serial0 = &uart_B;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x40000000>;
	};

};

&uart_B {
	status = "okay";
};
